num=[1,0,2,0,3,0,4]

n1=[]

n2=[]

for i in num:
    
if i==0:
        
   n1.append(i)
   
else:
       
   n2.append(i)

print(n2+n1)



num=[1,0,2,0,3,0,4]

n1=[]

n2=[]

for i in num:
    
if i==0:
       
 n1.append(i)
   
 else:
        
 n2.append(i)

k=len(n2)//2

p=n2[ :k]

p1=n2[k:]

print(p+n1+p1)


n=int(input("enter a number"))

if n//2==0:
    
  if n>6 and n<=20:
     
    print("wired")
   
  else:
        
    print("not wired")

else:
   
 print("wired")
    